{
    "manifest_version": 2,

    "name": "Clicks",
    "version": "1.0",


    "browser_action": {
        "default_title": "Hello, World"
    },

    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["content.js"]
      }
    ],

    "background": {
      "scripts": ["background.js"],
      "persistent": false
    }
}
